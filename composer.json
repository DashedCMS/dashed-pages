{
    "name": "dashed/dashed-pages",
    "description": "This is my package dashed-pages",
    "keywords": [
        "Dashed",
        "laravel",
        "dashed-pages"
    ],
    "homepage": "https://github.com/Dashed-DEV/dashed-pages",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@dashed.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.63",
        "orchestra/testbench": "^9",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Dashed\\DashedPages\\": "src",
            "Dashed\\DashedPages\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dashed\\DashedPages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashed\\DashedPages\\DashedPagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
